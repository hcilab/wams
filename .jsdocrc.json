{
  "markdown": {
    "idInHeadings": true
  },
  "opts": {
    "destination": "docs",
    "mainpagetitle": "Home | WAMS Documentation",
    "package": "package.json",
    "pedantic": true,
    "readme": "README.md",
    "recurse": true
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "css": ["styles/custom.css"],
    "default": {
      "outputSourceFiles": true,
      "useLongnameInNav": true
    },
    "footerText": "Workspaces Across Multiple Surfaces",
    "logo": {
      "width": "0px",
      "height": "0px"
    },
    "monospaceLinks": false,
    "name": "WAMS Documentation",
    "useCollapsibles": true
  }
}
