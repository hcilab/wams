{
  "markdown": {
    "idInHeadings": true
  },
  "opts": {
    "destination": "docs/",
    "encoding": "utf8",
    "mainpagetitle": "Home | WAMS Documentation",
    "package": "package.json",
    "pedantic": true,
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/docdash",
    "verbose": true
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "include": [
        "src",
        "index.js"
      ],
    "includePattern": ".js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "docdash": {
      "static": false,
      "sort": true,
      "sectionOrder": [
          "Modules",
          "Externals",
          "Events",
          "Namespaces",
          "Classes",
          "Mixins",
          "Tutorials",
          "Interfaces"
      ],
      "meta": {
          "title": "WAMS Documentation",
          "description": "Workspaces Across Multiple Surfaces",
          "keyword": "javascript"
      },
      "search": true,
      "wrap": true,
      "collapse": true,
      "typedefs": true,
      "private": false,
      "removeQuotes": "trim",
      "ShortenTypes": false,
      "scopeInOutputPath": false,
      "nameInOutputPath": false,
      "versionInOutputPath": false
  }
}
