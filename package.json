{
  "name": "wams",
  "description": "Workspaces Across Multiple Surfaces",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hcilab/wams"
  },
  "scripts": {
    "build": "parcel build",
    "build:debug": "parcel build --no-optimize",
    "lint": "eslint .",
